@/class'\n// import Base from '@/class/vegetable/base'\nimport Sunflower from '@/class/vegetable/sunflower'\n\nexport default {\n  name: 'game',\n  data(){\n    return {\n      game : new Game(),\n      vaultValue: 0\n    }\n  },\n  components:{\n    cell: () => import('./cell.vue'),\n  },\n  methods:{\n    costValue (value){\n      this.vaultValue-=value\n    },\n    clickRoom(x,y){\n      console.log(x,y)\n      let v = new Sunflower(1,100)\n      this.game.setVegetable(x,y,v)\n      this.$forceUpdate()\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.game{\n  position: absolute;\n  left: 255px;\n  top:100px\n}\n\n.row{\n  height: 45px;\n  display: flex;\n  align-items: flex-start;\n  justify-content: flex-start;\n}\n.cell{\n  display: inline-block;\n  height: 100%;\n  width: 39px;\n}\n</style>\n"]}]}